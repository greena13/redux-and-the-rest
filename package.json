{
  "name": "redux-and-the-rest",
  "version": "0.1.2",
  "description": "Declarative, flexible Redux integration with your RESTful API",
  "main": "index.js",
  "scripts": {
    "prepublish": "rm -rf cjs && npm run build-development && npm run build-production",
    "build-development": "BABEL_ENV=development NODE_ENV=development babel src --ignore tests -d cjs",
    "build-production": "BABEL_ENV=production NODE_ENV=production rollup -c",
    "build-tests": "rm -rf cjs && BABEL_ENV=test NODE_ENV=development babel src -d cjs",
    "tests": "npm run build-tests && jasmine",
    "watch-tests": "nodemon --exec 'npm run tests || true'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greena13/redux-and-the-rest.git"
  },
  "keywords": [
    "React",
    "Redux",
    "RESTful",
    "API"
  ],
  "files": [
    "cjs",
    "package.json",
    "README.md",
    "LICENSE",
    "index.d.ts",
    "index.js"
  ],
  "typings": "./index.d.ts",
  "author": "Aleck Greenham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/greena13/redux-and-the-rest/issues"
  },
  "homepage": "https://github.com/greena13/redux-and-the-rest#readme",
  "peerDependencies": {
    "isomorphic-fetch": "^2.2.1",
    "redux": ">= 1.0.0",
    "redux-thunk": ">= 1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.17.0",
    "fetch-mock": "^6.0.1",
    "jasmine": "^3.0.0",
    "node-fetch": "^2.0.0",
    "nodemon": "^1.14.12",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rollup": "^0.55.5",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "pluralize": "^7.0.0",
    "query-string": "^5.1.0"
  }
}
